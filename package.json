{
  "name": "flora-mysql",
  "version": "0.9.1",
  "description": "MySQL connection for Flora",
  "main": "index.js",
  "scripts": {
    "test:cleanup": "docker-compose -f test/integration/docker-compose.yml rm --force",
    "test:start-mysql": "docker-compose -f test/integration/docker-compose.yml up -d mysql",
    "test:mysql": "docker-compose -f test/integration/docker-compose.yml up --abort-on-container-exit test",
    "test:stop": "docker-compose -f test/integration/docker-compose.yml stop && docker-compose -f test/integration/docker-compose.yml kill",
    "test:_integration": "mocha test/integration/*.spec.js",
    "test:integration": "npm run test:cleanup && npm run test:start-mysql && npm run test:mysql; npm run test:stop",
    "test:unit": "mocha test/unit/*.spec.js",
    "test": "npm run test:unit && npm run test:integration",
    "lint": "eslint ."
  },
  "author": {
