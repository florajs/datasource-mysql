    "flora-errors": "^0.7.0",
    "flora-sql-parser": "^0.7.7",
    "has": "^1.0.1",
    "lodash": "^4.17.11",
    "mysql": "^2.15.0",
    "named-placeholders": "^1.1.1",
    "sqlstring": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/sinon-chai": "^3.2.0",
    "bunyan": "^1.5.1",
    "chai": "^4.2.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
